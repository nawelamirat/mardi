@model Formulaire.Models.Article
@{
    ViewBag.Title = "CreateArticle";
}

<h2>CreateFournisseur</h2>

@using (Html.BeginForm())
{
    <div>
        @Html.LabelFor(model => model.Nom)
        @Html.TextBoxFor(model => model.Nom)
    </div>
    <div>
        @Html.Label("Fournisseur")
        @Html.DropDownList("Fournisseur", (SelectList)ViewBag.ListeDesFournisseurs)
    </div>    

        <br />
        <input type="submit" value="Valider" />
    
}

